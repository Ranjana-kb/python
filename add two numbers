#include <iostream>
using namespace std;

int addUsingIncrement(int a, int b) {
    

    return a;
}

int main() {
    int a, b;
    cout << "Enter two integers: ";
    cin >> a >> b;
    for (int i = 0; i < b; i++) {
        a++;
    }
    for (int i = 0; i > b; i--) {
        a--;
    }
    cout << "Sum = " << a << endl;

    return 0;
}
